          <argument>
            <kind>1</kind>
            <string>///Pacman Movement
/*
   When not moving, check to see if a direction key is held.
   If so, assign x/y speed and change status to moving.
*/
//show_debug_message(SwipeTime);
if (isMoving == false)
{   
    //
    //right
    //
    if (move[0] == 1 )
    {
    if(!(place_meeting(x+8,y,obj_wall)) &amp;&amp; !(place_meeting(x+8,y,obj_side)))    //if theres no wall
    {
        isMoving = true;
        moveTimer = gridSize;
        speedX = moveSpeed;
        speedY = 0;
        startSwipe = false;
    }
    else
    {
    sprite_index = Pac_right_idle;
    
    if(startSwipe &amp;&amp; inputTimer &lt;= 0)
    {
    var inst;
        inst = instance_place(x+8, y, obj_wall);
        if inst != noone
        {   
            isMoving = true;
        moveTimer = gridSize*2;
        speedX = moveSpeed*0.5;
        speedY = 0;
        startSwipe = false;
        sprite_index = Pac_right_walk;
        inputTimer = 60;
        
            with (inst) 
            {
            state = wallStates.dying;         
            }
        }
    }
    }
    
    }

    //
    //up
    //
    if (move[1] == -1)
    {
        if(!(place_meeting(x,y-8,obj_wall)) &amp;&amp; !(place_meeting(x,y-8,obj_side))) 
    {
        isMoving = true;
        moveTimer = gridSize;
        speedX = 0;
        speedY = -moveSpeed;
        startSwipe = false;
    }
    else
    {
    sprite_index = Pac_up_idle;
    if(startSwipe &amp;&amp; inputTimer &lt;= 0)
    {
    var inst;
        inst = instance_place(x, y-8, obj_wall);
        if inst != noone
        {   
            isMoving = true;
        moveTimer = gridSize*2;
        speedX = 0;
        speedY = -moveSpeed*0.5;
        startSwipe = false;
        sprite_index = Pac_up_walk;
        inputTimer = 60;
        
            with (inst) 
            {
            state = wallStates.dying;         
            }
        }
    }
    }
    
    }
    
    //
    // left
    //
    if (move[0] == -1)
    {
        if(!(place_meeting(x-8,y,obj_wall)) &amp;&amp; !(place_meeting(x-8,y,obj_side))) 
    {
        isMoving = true;
        moveTimer = gridSize;
        speedX = -moveSpeed;
        speedY = 0;
        startSwipe = false;
    }
    else
    {
    sprite_index = Pac_left_idle;
    if(startSwipe &amp;&amp; inputTimer &lt;= 0)
    {
    var inst;
        inst = instance_place(x-8, y, obj_wall);
        if inst != noone
        {   
            isMoving = true;
        moveTimer = gridSize*2;
        speedX = -moveSpeed*0.5;
        speedY = 0;
        startSwipe = false;
        sprite_index = Pac_left_walk;
        inputTimer = 60;
        
            with (inst) 
            {
            state = wallStates.dying;         
            }
        }
    }
    }
    
    }
    
    //
    //down
    //
    if (move[1] == 1 )
    {
        if(!(place_meeting(x,y+8,obj_wall)) &amp;&amp; !(place_meeting(x,y+8,obj_side))) 
    {
        isMoving = true;
        moveTimer = gridSize;
        speedX = 0;
        speedY = moveSpeed;
        startSwipe = false;
    }
    else
    {
        sprite_index = Pac_down_idle;
    if(startSwipe &amp;&amp; inputTimer &lt;= 0)
    {
    var inst;
        inst = instance_place(x, y+8, obj_wall);
        if inst != noone
        {   
            isMoving = true;
        moveTimer = gridSize*2;
        speedX = 0;
        speedY = moveSpeed*0.5;
        startSwipe = false;
        sprite_index = Pac_down_walk;
        inputTimer = 60;
        
            with (inst) 
            {
            state = wallStates.dying;         
            }
        }
    }
    }
    
    }
}

/*
   When moving, subtract from moveTimer our moveSpeed value
   and update location relevant to set speeds.
   Stop moving when moveTimer reaches zero.
*/
if (isMoving == true)
{
   x += speedX;
   y += speedY
   
   moveTimer -= moveSpeed;
   if (moveTimer == 0) 
   {
   
   if(x &gt; xprevious)
   {
         sprite_index = Pac_right_idle;
    }
    else if(x &lt; xprevious)
    {
         sprite_index = Pac_left_idle;
    }
    else if(y &gt; yprevious)
    {
         sprite_index = Pac_down_idle;
    }
    else if(y &lt; yprevious)
    {
         sprite_index = Pac_up_idle;
    }
    else
    {
        move[0] = 0;
        move[1] = 0;
    }
   isMoving = false;
   
   }
}

 
